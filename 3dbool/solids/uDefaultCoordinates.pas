procedure InitDefaultCoordinates;
var
 i: Integer;
 l_oPoint3d: T3DBoolPoint3d;
begin
 g_lstDefaultBoxVertices := TList<T3DBoolPoint3d>.Create;
 for i := Low(DEFAULT_BOX_VERTICES) to High(DEFAULT_BOX_VERTICES) do begin
   l_oPoint3d := T3DBoolPoint3d.Create;
   l_oPoint3d.X := DEFAULT_BOX_VERTICES[i].x;
   l_oPoint3d.y := DEFAULT_BOX_VERTICES[i].y;
   l_oPoint3d.z := DEFAULT_BOX_VERTICES[i].z;
   g_lstDefaultBoxVertices.Add(l_oPoint3d);
 end;
 g_lstDefaultBoxCoordinates := TList<Cardinal>.Create;
